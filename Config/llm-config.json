{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "2.0.0",
  "providers": {
    "pcai-inference": {
      "enabled": true,
      "baseUrl": "http://127.0.0.1:8080",
      "defaultModel": "pcai-inference",
      "timeout": 120000
    },
    "functiongemma": {
      "enabled": true,
      "baseUrl": "http://127.0.0.1:8000",
      "defaultModel": "functiongemma-270m-it",
      "timeout": 120000
    }
  },
  "contextManagement": {
    "maxContextTokens": 16384,
    "reservedForResponse": 4096,
    "systemPromptFile": "DIAGNOSE.md",
    "decisionTreeFile": "DIAGNOSE_LOGIC.md",
    "chatPromptFile": "CHAT.md"
  },
  "router": {
    "enabled": true,
    "provider": "functiongemma",
    "baseUrl": "http://127.0.0.1:8000",
    "model": "functiongemma-270m-it",
    "toolsPath": "Config/pcai-tools.json"
  },
  "prompts": {
    "diagnosticAnalysis": {
      "temperature": 0.3,
      "maxTokens": 4096,
      "topP": 0.9
    },
    "optimization": {
      "temperature": 0.5,
      "maxTokens": 2048,
      "topP": 0.95
    }
  },
  "responseFormat": {
    "includeReasoningSteps": true,
    "structuredOutput": true,
    "markdownFormatting": true
  },
  "fallbackOrder": [
    "pcai-inference"
  ]
}
